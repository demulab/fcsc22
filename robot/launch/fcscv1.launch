<launch>
    <include file="$(find robot)/launch/vacuum.launch"/>
    <!--include file="$(find robot)/launch/mikiwame.launch"/-->
    <include file="$(find robot)/launch/ar_mikiwame.launch"/>
    <include file="$(find scout_base)/launch/scout_mini_omni_base.launch"/>
    <include file="$(find dynamixel_wrs2021)launch/motor_setup.launch"/>

    <node pkg="dynamixel_wrs2021" type="motor_controller.py" name="moto_controller"/>

    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="/dev/ttyARDUINO"/>
        <param name="baund" value="57600"/>
    </node>
</launch>    
